extends layout.pug

block content
  section
    h1 Notes
    p Test
    ul
      if coursesInfo
        each note in coursesInfo
          li
            if note.date
              span #{note.date}
            a(href=`${note.link}`) #{note.title? note.title: "Test"}
      if coursesConcepts
        each note in coursesConcepts
          li
            if note.date
              span #{note.date}
            a(href=`${note.link}`) #{note.title? note.title: "Test"}

